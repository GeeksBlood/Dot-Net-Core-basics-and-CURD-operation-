@model CoreDemo.Models.DB.Reg



<div class="container">
    <h2>Details by using view component</h2>
    <div class="form-group">
        <label for="Name">Name:</label>
        @Model.Name
    </div>
    <div class="form-group">
        <label for="Email">Email:</label>
        @Model.Email

    </div>
    <div class="form-group">
        <label for="Mob">Mobile No.:</label>
        @Model.Mob
    </div>
    <div class="form-group">
        <label for="Address">Address:</label>
        @Model.Address
    </div>
</div>